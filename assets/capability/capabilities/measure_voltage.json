{
  "type": "number",
  "title": {
    "en": "Voltage",
    "nl": "Voltage"
  },
  "units": {
    "en": "V"
  },
  "desc": {
    "en": "Voltage (V)",
    "nl": "Voltage (V)"
  },
  "options": {
    "isRootOfZone": {
      "type": "string",
      "default": "",
      "desc": {
        "en": "This option contains a zone identifier and indicates that this measurement is a root measurement for that zone"
      }
    }
  }
  "chartType": "stepLine",
  "decimals": 2,
  "getable": true,
  "setable": false,
  "uiComponent": "sensor",
  "$flow": {
    "triggers": [
      {
        "id": "measure_voltage_changed",
        "title": {
          "en": "The voltage changed",
          "nl": "Het voltage is veranderd"
        },
        "tokens": [
          {
            "name": "$id",
            "title": "$title",
            "type": "$type",
            "example": 7.5
          }
        ]
      }
    ]
  }
}
